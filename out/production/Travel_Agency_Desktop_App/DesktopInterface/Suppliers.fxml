<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="suppliers" prefHeight="463.0" prefWidth="709.0" stylesheets="@css/desktopStyling.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DesktopInterface.SuppliersController">
    <children>
        <Label prefHeight="17.0" prefWidth="375.0" text="Suppliers Page">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
      <Button fx:id="btnAddProductToSupplier" layoutX="74.0" layoutY="270.0" mnemonicParsing="false" onAction="#addProdToSupp" prefHeight="25.0" prefWidth="227.0" text="Add Selected Product to Supplier" />
      <Button fx:id="btnRemoveProductToSupplier" layoutX="402.0" layoutY="270.0" mnemonicParsing="false" onAction="#removeProdFromSupp" prefHeight="25.0" prefWidth="227.0" text="Remove Selected Product from Supplier" />
      <ComboBox fx:id="cmbSupplier" layoutX="75.0" layoutY="44.0" onAction="#selectSupplier" prefHeight="25.0" prefWidth="227.0" promptText="Select a Supplier" />
      <ListView fx:id="lstProductBySupplier" layoutX="402.0" layoutY="92.0" prefHeight="171.0" prefWidth="227.0" />
      <ListView fx:id="lstProductToAdd" layoutX="75.0" layoutY="92.0" prefHeight="171.0" prefWidth="227.0" />
      <Label layoutX="23.0" layoutY="48.0" text="Supplier:" />
      <Label layoutX="402.0" layoutY="75.0" text="Products by Supplier:" />
      <Label layoutX="75.0" layoutY="75.0" text="Products to Add to Supplier:" />
      <TitledPane fx:id="crudSuppliers" animated="false" layoutX="188.0" layoutY="316.0" prefHeight="133.0" prefWidth="333.0" text="Add, Edit, Remove Supplier">
         <content>
            <Pane prefHeight="87.0" prefWidth="331.0">
               <children>
                  <Button fx:id="btnRemoveSupplier" layoutX="208.0" layoutY="70.0" mnemonicParsing="false" onAction="#removeSupplier" text="Remove Supplier" />
                  <Button fx:id="btnEditSupplier" layoutX="208.0" layoutY="40.0" mnemonicParsing="false" onAction="#editSupplier" text="Edit Supplier" />
                  <TextField fx:id="txtSupplier" layoutX="14.0" layoutY="41.0" prefHeight="25.0" prefWidth="181.0" />
                  <Label layoutX="14.0" layoutY="14.0" text="Supplier Name:" />
                  <Button fx:id="btnAddSupplier" layoutX="208.0" layoutY="10.0" mnemonicParsing="false" onAction="#addSupplier" text="Add New Supplier" />
                  <Label fx:id="lblError" layoutX="14.0" layoutY="62.0" text="Error Text" />
               </children>
            </Pane>
         </content>
      </TitledPane>
    </children>
</AnchorPane>
